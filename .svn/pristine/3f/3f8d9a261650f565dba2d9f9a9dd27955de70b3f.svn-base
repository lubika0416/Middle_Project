package vo;


public class CouponVO {
	
	private int couponNo;
	
	private String couponContent;
	
	private int couponUsed = 0; // 미사용 0 사용 1
	
	private int couponDisc;
	
	private String memId;
	
	private int couponPeriod;
	
	public CouponVO() {}

	public CouponVO(String couponContent,int couponDisc, String memId) {
		this.couponContent = couponContent;
		this.couponDisc = couponDisc;
		this.memId = memId;
	}

	public int getCouponNo() {
		return couponNo;
	}

	public void setCouponNo(int couponNo) {
		this.couponNo = couponNo;
	}

	public String getCouponContent() {
		return couponContent;
	}

	public void setCouponContent(String couponContent) {
		this.couponContent = couponContent;
	}

	public int getCouponUsed() {
		return couponUsed;
	}

	public void setCouponUsed(int couponUsed) {
		this.couponUsed = couponUsed;
	}

	public int getCouponDisc() {
		return couponDisc;
	}

	public void setCouponDisc(int couponDisc) {
		this.couponDisc = couponDisc;
	}

	public String getMemId() {
		return memId;
	}

	public void setMemId(String memId) {
		this.memId = memId;
	}

	public int getCouponPeriod() {
		return couponPeriod;
	}

	public void setCouponPeriod(int couponPeriod) {
		this.couponPeriod = couponPeriod;
	}

	@Override
	public String toString() {
		return "CouponVO [couponNo=" + couponNo + ", couponContent=" + couponContent + ", couponUsed=" + couponUsed
				+ ", couponDisc=" + couponDisc + ", memId=" + memId + ", couponPeriod=" + couponPeriod + "]";
	}
	
	
	
	
	
	
}
